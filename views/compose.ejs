<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post">

  <div class="form-group">
    <label for="composeTextTitle">Title</label>
    <input  class="form-control" type="text" id="composeTextTitle" name="postTitle">
  </div>
  <div class="form-group">
    <label for="composeTextPost">Post</label>
    <textarea class="form-control" name="postBody" id="composeTextPost" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>



<%- include("partials/footer"); -%>
